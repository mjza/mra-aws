{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:GetObjectTagging",
                "s3:PutObjectTagging",
                "s3:PutObjectAcl",
                "s3:ListBucket",
                "s3:DeleteObject",
                "tag:TagResources"
            ],
            "Resource": [
                "arn:aws:s3:::mra-public-bucket/*",
                "arn:aws:s3:::mra-public-bucket/**/*",
                "arn:aws:s3:::mra-public-bucket/",
                "arn:aws:s3:::mra-private-bucket/*",
                "arn:aws:s3:::mra-private-bucket/**/*",
                "arn:aws:s3:::mra-private-bucket/"
            ]
        }
    ]
}